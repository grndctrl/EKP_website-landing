body,
html {
  @apply text-black bg-white font-sans;
}

header.header-main {
  @apply fixed w-full z-50;
}

main.main {
  flex: 1 0 auto;
  padding-top: 120px;
  min-height: 100vh;
}

footer.footer-main {
  flex-shrink: 0;
}

a,
a:active,
a:focus,
a:visited {
  @apply text-black no-underline;
}

a:hover {
  @apply underline;
}
