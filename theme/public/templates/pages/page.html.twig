{% extends "core/base.html.twig" %}

{% set main_class = ['header-offset'] %}

{% block main %}

  {% embed "layout/section.html.twig" %}

    {% block section %}

      <div class="row">
        <div class="col w-full md:w-10/12 lg:w-8/12 xl:w-6/12">
          <h2>{{ post.title }}</h2>
          <p>{{ post.content }}</p>
        </div>
      </div>

    {% endblock %}

  {% endembed %}

{% endblock %}
